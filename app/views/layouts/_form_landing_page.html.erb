<%=  simple_form_for :bike, :html => { :class => 'form-inline' } do |f|  %>

    <%= f.input :city, placeholder: 'City', label: false %>

    <%= f.input :start_date, placeholder: 'Start Date', label: false %>

    <%= f.input :end_date, placeholder: 'End Date', label: false %>

    <%= f.submit "Find a bike", class: "btn btn-warning" %>


<% end %>
